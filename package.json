{
  "name": "bulma-styled-components",
  "version": "2.1.0",
  "description": "A Styled Components implementation of Bulma Css for React.",
  "main": "dist/index.js",
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "lint:style": "stylelint './src/**/*.js'",
    "lint:js": "eslint 'src'",
    "lint": "npm run lint:js && npm run lint:style",
    "docs": "catalog start",
    "predeploy": "npm run build:docs",
    "deploy": "gh-pages -d catalog/build",
    "build:docs": "catalog build -u './'",
    "prepare": "npm run build",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mpaupulaire4/bulma-styled-components.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "bulma",
    "emotion",
    "styled",
    "components",
    "react",
    "style"
  ],
  "author": {
    "name": "Michel Paupulaire",
    "url": "https://github.com/mpaupulaire4"
  },
  "license": "MIT",
  "dependencies": {
    "emotion": "^9.2.10",
    "polished": "^1.9.2"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-plugin-emotion": "^9.2.10",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "catalog": "^3.6.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "gh-pages": "^1.2.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.0",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^3.0.5",
    "rollup-plugin-commonjs": "^9.1.8",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "stylelint": "^9.2.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.1",
    "uglify-js": "^3.4.9",
    "webpack": "^4.15.1"
  }
}
